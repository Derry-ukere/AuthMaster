{
  "name": "distinct-web-backend",
  "version": "1.0.0",
  "description": "Distinct backend API",
  "main": "./src/bin/www/index.ts",
  "private": true,
  "scripts": {
    "prod": "node ./dist/bin/www/index.js",
    "dev": "nodemon ./src/bin/www/index.ts",
    "build": "webpack",
    "test": "jest",
    "test-watch": "jest --watchAll --collectCoverage=false",
    "prettier-watch": "onchange './src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "clean": "rm -rf coverage dist",
    "typings": "node ./specifications/lib/merger.js && openapi --input ./specifications/resolved/api-gateway.json --output ./src/typings",
    "validate-specs": "for filename in ./specifications/specs/*; do [ $filename != ./specifications/specs/_* ] && echo \"Validating file ($filename)\" && node_modules/.bin/lint-openapi $filename; done",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -f ./src/typeorm.ts"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "license": "LicenseRef-LICENSE",
  "author": {
    "name": "Distinct Developers",
    "email": "developers@distinct.ai"
  },
